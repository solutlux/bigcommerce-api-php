{
    "name": "solutlux/bigcommerce-api-php",
    "type": "library",
    "description": "Enables PHP applications to communicate with the Bigcommerce v3 API.",
    "keywords": [
        "api",
        "http",
        "rest",
        "ecommerce",
        "business"
    ],
    "homepage": "http://developer.bigcommerce.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Bigcommerce",
            "homepage": "http://www.bigcommerce.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "vlucas/phpdotenv": "1.0.6",
        "behat/behat": "2.4.*@stable",
        "guzzlehttp/guzzle": "*",
        "bigcommerce/api": "*"
    },
    "autoload": {
        "psr-0": {
            "Bigcommerce": "src/"
        }
    },
    "archive": {
        "exclude": [
            "bin"
        ]
    }
}
